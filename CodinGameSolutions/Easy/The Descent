using System;
using System.Linq;
using System.IO;
using System.Text;
using System.Collections;
using System.Collections.Generic;


class Player
{
    static void Main(string[] args)
    {
        while (true)
        {
            int heighest_mountain = 0;
            int index = 0;

            for (int i = 0; i < 8; i++)
            {
                int mountainH = int.Parse(Console.ReadLine()); 
                
                if(mountainH > heighest_mountain){
                    heighest_mountain = mountainH;
                    index = i;
                }
            }

            Console.WriteLine(index); 
            heighest_mountain = 0;
        }
    }
}
